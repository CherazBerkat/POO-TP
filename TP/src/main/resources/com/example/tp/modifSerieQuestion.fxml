<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.tp.ModifSerieQuestionController"
            prefHeight="600.0" prefWidth="900.0">
    <children>
        <BorderPane prefHeight="600.0" prefWidth="900.0">
            <center>
                <ListView fx:id="listQ" prefHeight="518.0" prefWidth="900.0" BorderPane.alignment="CENTER" />
            </center>
            <top>
                <HBox fx:id="upHbox" prefHeight="100.0" prefWidth="900.0" BorderPane.alignment="CENTER">
                    <BorderPane.margin>
                        <Insets top="4.0" />
                    </BorderPane.margin>
                </HBox>
            </top>
            <bottom>
                <Button mnemonicParsing="false" onAction="#addQuest" prefHeight="26.0" prefWidth="156.0" style="-fx-background-color: #48efa6; -fx-font-weight: 700;" text="Ajouter Question" textFill="WHITE" BorderPane.alignment="BOTTOM_RIGHT">
                    <BorderPane.margin>
                        <Insets bottom="10.0" right="10.0" top="10.0" />
                    </BorderPane.margin>
                </Button>
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
